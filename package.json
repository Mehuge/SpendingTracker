{
  "name": "spendingtracker",
  "version": "1.0.0",
  "description": "Track your spending month by month",
  "main": "index.js",
  "scripts": {
    "test": "test.js",
    "init-registry": "npm config set registry http://registry.npmjs.org/",
    "install-globals": "npm install -g rimraf",
    "init": "npm run install-globals && rimraf node_modules && npm run init-registry && npm install && npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack"
  },
  "keywords": [
    "spending",
    "tracker"
  ],
  "author": "Mehuge",
  "license": "MIT",
  "repository": "Mehuge/SpendingTracker",
  "dependencies": {
    "@types/react": "^15.0.1",
    "@types/react-dom": "^0.14.20",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "source-map-loader": "^0.1.6",
    "typescript": "=2.1.1"
  }
}
